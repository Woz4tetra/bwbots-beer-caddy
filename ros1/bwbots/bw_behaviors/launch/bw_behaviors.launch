<launch>
    <group ns="bw">
        <node name="bw_commands" pkg="bw_behaviors" type="actions_node.py" output="screen"
            required="false">
            <rosparam file="$(find bw_behaviors)/config/commands.yaml" command="load" ns="" />
            <remap from="cmd_vel" to="cmd_vel_nav" />
            <param name="class_names_path" value="$(find bw_yolo)/models/person.names" />
        </node>
        <node name="bw_bts" pkg="bw_behaviors" type="trees_node.py" output="screen" required="false">
            <rosparam file="$(find bw_behaviors)/config/tag_mapping.yaml" command="load"
                ns="tag_mapping" />
            <remap from="cmd_vel" to="cmd_vel_nav" />
        </node>
    </group>
</launch>